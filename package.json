{
  "name": "markdown-or-chalk",
  "version": "0.2.3",
  "description": "A GitHub template repo for node modules",
  "homepage": "http://github.com/voxpelli/markdown-or-chalk",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/markdown-or-chalk.git"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "exports": "./index.js",
  "types": "index.d.ts",
  "files": [
    "/index.js",
    "/index.d.ts",
    "/index.d.ts.map",
    "lib/*.js",
    "lib/*.d.ts",
    "lib/*.d.ts.map"
  ],
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build": "run-s build:*",
    "check:installed-check": "installed-check -i @voxpelli/eslint-config -i eslint -i knip -i installed-check -i npm-run-all2",
    "check:knip": "knip",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:tsc": "tsc",
    "check:type-coverage": "type-coverage --detail --strict --at-least 95 --ignore-files 'test/*'",
    "check": "run-s clean && run-p check:*",
    "clean:declarations-top": "rm -rf $(find . -maxdepth 1 -type f -name '*.d.ts*' ! -name 'advanced-types.d.ts' ! -name 'index.d.ts')",
    "clean:declarations-lib": "rm -rf $(find lib -type f -name '*.d.ts*' ! -name 'advanced-types.d.ts')",
    "clean": "run-p clean:*",
    "prepare": "husky",
    "prepublishOnly": "run-s build",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*"
  },
  "dependencies": {
    "@types/mdast": "^4.0.3",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "is-unicode-supported": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "mdast-util-to-string": "^4.0.0",
    "strip-ansi": "^7.1.0",
    "terminal-link": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.9",
    "@types/node": "^18.19.59",
    "@voxpelli/eslint-config": "^22.0.0",
    "@voxpelli/tsconfig": "^15.0.0",
    "c8": "^10.1.2",
    "chai": "^4.5.0",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "installed-check": "^9.3.0",
    "knip": "^5.34.1",
    "mocha": "^10.7.3",
    "npm-run-all2": "^7.0.1",
    "type-coverage": "^2.29.7",
    "typescript": "~5.6.3",
    "validate-conventional-commit": "^1.0.4"
  }
}
